uqbwoodc@fraser:20141007:~/git/graftM/test/data/1_small_16S_tree$ taxit create --seq-info Seq.csv --tree-file 82_otus.fasta.tre --tree-stats fasttree.log --taxonomy Tax.csv -l 16S_gg201308_82 -P 16S_gg201308_82.refpkg -c --aln-fasta /srv/db/gg/2013_08/gg_13_8_otus/rep_set_aligned/82_otus.fasta
